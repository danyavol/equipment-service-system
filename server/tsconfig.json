{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "outDir": "dist",
        "rootDir": "src",
        "baseUrl": ".",
        "strict": true,
        "esModuleInterop": true,
        "paths": {
            "@models/*": [ "src/models/*" ],
        },
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
    },
    "exclude": [
        "./node_modules"
    ]
}
